<% include partials/header %>
    <h1 class="text-center">Add Campground</h1>
    <div class="row justify-content-center">
      <div class="col-md-6">
        <form action="/campgrounds" method="post">
          <div class="form-group">
            <input type="text" name="campName" class="form-control" placeholder="Camp Name"/>
          </div>
          <div class="form-group">
            <input type="text" name="campImage" class="form-control" placeholder="Camp Image URL"/>
          </div>
          <button type="submit" class="btn btn-primary btn-block">Submit</button>
          <button type="submit" class="btn btn-outline-secondary btn-block">Cancel</button>
        </form>
      </div>
    </div>
<% include partials/footer %>